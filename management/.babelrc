{
  "presets": ["env", "react", "stage-2"],
  "plugins": [
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true}],
    "react-hot-loader/babel",
    "transform-decorators-legacy",
    "syntax-dynamic-import",
    [
      "transform-runtime",
      {
        "helpers": false,
        "polyfill": false,
        "regenerator": true,
        "moduleName": "babel-runtime"
      }
    ]
  ]
}